<h1>Create a new song!</h1>
<form action="/songs" method="POST">
    <label for="song[name]">Song Name </label>
    <input type="text" name="song[name]" id="Name">
    <br>
    <label for="artist[name]">Artist Name </label>
    <input type="text" name="artist[name]" id="Artist Name">
    <br>
    <%@genres.each do |genre|%>
            <label for="song[genres][][id]"> <%=genre.name%>:</label>
            <input type="checkbox" name="genres[][id]" value="<%= genre.id %>" id="<%=genre.name%>">
            <br>
    <% end %>

    <input type="submit" id="Create" value="Change">
<form>
